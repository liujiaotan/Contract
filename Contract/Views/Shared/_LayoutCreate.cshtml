@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="page-header">
    <h3>@ViewBag.Title <small>新建@(ViewBag.Title)</small></h3>
</div>
@{
    ViewBag.Title = ViewBag.Title + " - " + Resources.Resource.Create;
}
@RenderBody()
@section scripts{
    @RenderSection("scripts", required: false)
}
@section styles{
    @RenderSection("styles", required: false)
} 

